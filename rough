
		try {
			while ((line = reader.readLine()) != null) {
				

				if (line.contains("switch (true) {")) {
					isSwitchTrue = true;
					line = "";
					result+="if(true){\n";
				}

				if (isSwitchTrue) {
					if (line.contains("case:")) {
						result += line.replace("case:", "if(")+"){\n";
					}

					if (line.contains("break;")) {
						result += "{\n";
						isSwitchTrue = false;
					}
				}



				result += line + "\n";

				// ... existing code ...
			}
