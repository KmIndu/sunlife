// Assuming 'line' is the VB6 code line
if (line.contains("case \"") && (line.contains("\" To \"") || line.contains("\" To")) {
    // Extract the letter range from the line
    String range = line.substring(line.indexOf("case \"") + 7, line.indexOf("\" To"));

    // Split the range into start and end characters
    String[] rangeParts = range.split("\"");
    if (rangeParts.length == 3) {
        char startChar = rangeParts[1].charAt(0);
        char endChar = rangeParts[2].charAt(0);

        // Replace VB6 Case range with individual Java cases
        StringBuilder javaCases = new StringBuilder();
        for (char currentChar = startChar; currentChar <= endChar; currentChar++) {
            javaCases.append("case \"").append(currentChar).append("\":\n");
        }

        // Replace the entire 'Case' statement with Java cases
        line = line.replace("case \"" + range + "\"", javaCases.toString());

        // Remove the trailing colon from the last case
        line = line.replaceAll(":\\s*$", "");
    }
}

// Example: Convert "case \"A\" To \"I\"" or "case \"A\" To" to individual Java cases
// case "A":
// case "B":
// ...
// case "I"






// Assuming 'line' is the VB6 code line
if (line.contains("Case \"") && line.contains("To \"")) {
    // Extract the letter range from the line
    String range = line.substring(line.indexOf("Case \"") + 7, line.indexOf("\" To"));
    char startChar = range.charAt(0);
    char endChar = range.charAt(range.length() - 1);

    // Replace VB6 Case range with individual Java cases
    StringBuilder javaCases = new StringBuilder();
    for (char currentChar = startChar; currentChar <= endChar; currentChar++) {
        javaCases.append("case \"").append(currentChar).append("\":\n");
    }

    // Replace the original line with Java cases
    line = javaCases.toString();
}




VB6-
Dim intLen As Integer
    Dim intSign As Integer
    Dim dblOut As Double
    Dim strLast As String * 1
    Dim intLast As Integer
' Decide how to convert the last character
    Select Case strLast
        Case "A" To "I"
            intSign = 1
            intLast = Asc(strLast) - 65 + 1
        Case "J" To "R"
            intSign = -1
            intLast = Asc(strLast) - 74 + 1
        Case "{"
            intSign = 1
        Case "}"
            intSign = -1
        Case Else
            intSign = 1
            intLast = 9
            strNum = "9999999999999"
    End Select



Java-
int intLen = 0;
        int intSign = 0;
        double dblOut = 0;
        String strLast = ""; * 1
        int intLast = 0;
switch (strLast) {
        case  "A" To "I":
        intSign = 1;
        intLast = (int)(strLast) - 65 + 1;

        case  "J" To "R":
        intSign = -1;
        intLast = (int)(strLast) - 74 + 1;

        case  "{":
        intSign = 1;

        case  "}":
        intSign = -1;
